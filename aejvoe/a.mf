w = 80;

x1 = x6 = 0;
x2 = w;
x3 = x4 = w / 2;

y1  =  y2 = h;
y3 = h * 2 / 3;
y4 = y7 = h * 1 / 3;
y5 = h / 2;
y6 = 0;

